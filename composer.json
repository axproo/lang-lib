{
    "name": "axproo/lang-lib",
    "version": "1.1.2",
    "description": "Biblioth√®que de gestion des langues multilingue pour CI4",
    "type": "composer-plugin",
    "require": {
        "php": "^8.1",
        "codeigniter4/framework": "^4.5",
        "composer-plugin-api": "^2.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Axproo\\LangLib\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Christian DJOMOU",
